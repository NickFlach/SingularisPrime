{
  "info": {
    "name": "ParadoxResolver API",
    "description": "Collection for testing ParadoxResolver OS endpoints in SINet",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "item": [
    {
      "name": "Get ParadoxResolver Status",
      "request": {
        "method": "GET",
        "url": {
          "raw": "{{host}}/api/paradox/status",
          "host": ["{{host}}"],
          "path": ["api", "paradox", "status"]
        },
        "description": "Check if ParadoxResolver OS is ready and connected to PlanetaryQuantumNetwork"
      },
      "response": []
    },
    {
      "name": "Activate Planetary Quantum Network",
      "request": {
        "method": "POST",
        "url": {
          "raw": "{{host}}/api/paradox/planetary-quantum-network/activate",
          "host": ["{{host}}"],
          "path": ["api", "paradox", "planetary-quantum-network", "activate"]
        },
        "description": "Activate the connection to the Planetary Quantum Network"
      },
      "response": []
    },
    {
      "name": "Get Planetary Quantum Network Status",
      "request": {
        "method": "GET",
        "url": {
          "raw": "{{host}}/api/paradox/planetary-quantum-network/status",
          "host": ["{{host}}"],
          "path": ["api", "paradox", "planetary-quantum-network", "status"]
        },
        "description": "Check the current status of the Planetary Quantum Network connection"
      },
      "response": []
    },
    {
